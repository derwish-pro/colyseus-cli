{
  "name": "colyseus-cli",
  "version": "0.1.0",
  "description": "CLI for remote control colyseus server",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"tsc -w\" \"nodemon dist/app.js\"",
    "postinstall": "npm run build",
    "check-updates": "ncu -u"
  },
  "author": "Derwish (derwish.pro@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.11.0"
  },
  "devDependencies": {
    "@types/commander": "^2.9.1",
    "concurrently": "^3.5.0",
    "typescript": "^2.1.4"
  }
}
